local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Obby 1% of win", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest",IntroText = "camera simp studios"})

local Tab = Window:MakeTab({
	Name = "farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

-- variables

getgenv().mn = true

-- works

function money()
while getgenv().mn == true do

local args = {
    [1] = 10000
}

game:GetService("ReplicatedStorage"):WaitForChild("Honeypot"):WaitForChild("Internal"):WaitForChild("RemoteStorage"):WaitForChild("AwardCash - RemoteEvent"):FireServer(unpack(args))

wait(.)

end
end

-- toggled
Tab:AddToggle({
	Name = "Auto Money",
	Default = false,
	Callback = function(Value)
		getgenv().mn = Value
      money()
	end    
})
OrionLib:Init()
